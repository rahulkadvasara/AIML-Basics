   "source": [
    "Write a Numpy program to find the indices of sorted elements of a given array"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Sorted elements: [1 2 3 4 5]\n",
      "Indices of sorted elements: [1 2 0 3 4]\n"
     ]
    }
   ],
   "source": [
    "arr = np.array([3, 1, 2, 4, 5])\n",
    "sorted_indices = np.argsort(arr)\n",
    "print(\"Sorted elements:\", arr[sorted_indices])\n",
    "print(\"Indices of sorted elements:\", sorted_indices)"
   ]
  }
 ],
